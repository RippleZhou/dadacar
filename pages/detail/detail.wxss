/* pages/detail/detail.wxss */

.list-banner-wrap {
  position: relative;
}

.list-banner {
  height: 420rpx;
  display: flex;
}

.slide-block-wrap {
  width: 60rpx;
  height: 36rpx;
  position: absolute;
  right: 30rpx;
  bottom: 20rpx;
  z-index: 1;
  background: rgba(0, 0, 0, 0.30);
  border-radius: 100rpx;
}

.slide-block {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 34rpx;
  height: 30rpx;
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #fff;
}

.list-box {
  border: none;
  padding: 0;
  margin: 0;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
}

.banner-image-wrap {
  flex: 1;
}

.banner-image {
  width: 100%;
}

.banner-default-image {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 22;
  transform: translate(0%, 0%) translateZ(0px);
  will-change: auto;
}

.repair-name {
  width: 100%;
  font-size: 32rpx;
  font-weight: bold;
  display: block;
  padding-top: 20rpx;
  box-sizing: border-box;
  color: #363636;
}

.repair-name-companyName {
  width: 100%;
  font-size: 36rpx;
  font-weight: bold;
  display: block;
  padding-top: 20rpx;
