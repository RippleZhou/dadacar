}

.list-bottom {
  background: #4081d6;
  height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  line-height: 100rpx;
  position: fixed;
  bottom: 0;
  width: 100%;
  border-radius: 0;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  /* opacity:0.5;  */
  background-color: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  color: #fff;
  z-index: 888;
}

.modal-dialog {
  width: 540rpx;
  height: 350rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 999;
  background: #f9f9f9;
  margin: -180rpx 105rpx;
  border-radius: 10rpx;
}

.modal-content {
  color: #363636;
  font: 36rpx "microsoft yahei";
  font-weight: 700;
  text-align: center;
  padding-top: 28%;
  /* position:absolute;
 top:50%;
 left:50%;
 transform: translate(-50%,-50%);  */
}

.btn-cancel {
  color: #363636;
}

.btn-confirm {
  color: #0f0;
  font: 32rpx "microsoft yahei";
  /* font-family: PingFangSC-Regular; */
  padding: 20rpx;
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
}
