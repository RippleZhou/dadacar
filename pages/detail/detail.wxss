  border-radius: 2px;
  box-sizing: border-box;
  border-bottom: none;
  font-size: 28rpx;
  margin-top: 30rpx;
}

.list-row {
  /* width: 100%; */
  display: flex;
  display: -webkit-flex;
  border-bottom: 1rpx solid #e4e4e4;
  padding: 30rpx;
  box-sizing: border-box;
  /* justify-content: space-around;  */
}

.list-td {
  flex: 0 0 50%;
  /* text-align: center;  */
}

.list-td-service {
  flex: 0 0 368rpx;
}

